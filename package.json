  },
  "homepage": "https://github.com/feyi-tech/flashbots-builder#readme",
  "devDependencies": {
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0"
  },
